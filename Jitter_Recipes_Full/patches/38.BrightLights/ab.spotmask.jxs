<jittershader name="blank">	<description>	shader program that does absolutely nothing!	</description>
		<param name="spot" type="vec2" default="50. 50." />	<param name="radius" type="float" default="10." />	<param name="fade" type="float" default="1." />	<param name="shape" type="vec2" default="1. 1." />	<param name="tex0" type="int" default="0" />	<language name="glsl" version="1.0">		<bind param="spot" program="fp" />		<bind param="radius" program="fp" />		<bind param="shape" program="fp" />		<bind param="fade" program="fp" />		<bind param="tex0" program="fp" />		<program name="vp" type="vertex" source="sh.passthrudim.vp.glsl" />		<program name="fp" type="fragment"><![CDATA[
//the essential stuff	uniform sampler2DRect tex0;
	varying vec2 texcoord0;	//our special stuff	uniform vec2 spot;	uniform vec2 shape;	uniform float radius;	uniform float fade;
void main(){   		float rad= distance(spot/shape,texcoord0/shape);	//vec2 spit = (spot-texcoord0)/vec2(1.,shape);	//float rad = sqrt(dot(spit,spit));		float mask = 1.-smoothstep(radius,radius+fade,rad);	vec4 texl= texture2DRect(tex0,texcoord0);	gl_FragColor = vec4(texl.rgb,mask);}]]>		</program>	</language></jittershader>